{
   "stage": 2,
   "env": {
      "development": {
         "plugins" : [
            "react-transform"
         ],
         "extra"  : {
            "react-transform": [
               {
                  "transform"  : "react-transform-hmr",
                  "imports" : ["react"],
                  "locals"  : ["module"]
               }, {
                  "transform": "react-transform-catch-errors",
                  "imports": ["react", "redbox-react"]
               }
            ]
         }
      },
      "production": {
         "plugins" : [
            "react-transform"
         ],
         "extra"  : {
            "react-transform": [
               {
                  "transform"  : "react-transform-hmr",
                  "imports" : ["react"],
                  "locals"  : ["module"]
               }, {
                  "transform": "react-transform-catch-errors",
                  "imports": ["react", "redbox-react"]
               }
            ]
         }
      }
   }
}
